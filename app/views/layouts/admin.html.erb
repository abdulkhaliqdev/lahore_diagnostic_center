<!DOCTYPE html>
<html>
  <head>
    <title>LahoreDiagnosticCenter--Admin</title>
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag "admin", "data-turbo-track": "reload" %>
    <%= javascript_pack_tag 'admin', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <div class="layout-wrapper layout-content-navbar">
      <div class="layout-container">
      <%= render 'admin/partials/sidebar' %>
      
      <div class="layout-page">
        <%= render 'admin/partials/header' %>
          <div class="content-wrapper">
            <%= yield %>
          </div>
        </div>
      </div>
    </div>
  </body>
</html>
