        <i class="menu-icon tf-icons bx bx-home-circle"></i>
        <div data-i18n="Analytics">Patients</div>
      <% end %>
    </li>
    <li class="menu-item <%= 'active' if params[:controller] == "admin/procedures" %>">
      <%= link_to admin_procedures_path, class: "menu-link" do %>
        <i class="menu-icon tf-icons bx bx-home-circle"></i>
        <div data-i18n="Analytics">Procedure</div>
      <% end %>
    </li>
    <li class="menu-item <%= 'active' if params[:controller] == "admin/referrers" %>">
      <%= link_to admin_referrers_path, class: "menu-link" do %>
        <i class="menu-icon tf-icons bx bx-home-circle"></i>
        <div data-i18n="Analytics">Referrers</div>
      <% end %>
    </li>
    <li class="menu-item <%= 'active' if params[:controller] == "admin/users" %>">
